    "start": "node ./dist/main.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KevinBacas/sobriquet.git"
  },
  "author": "Kevin Bacas <bacas.kevin@hotmail.fr>",

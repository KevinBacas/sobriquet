{
  "name": "sobriquet",
  "version": "1.0.2",
  "description": "A name generator based on Markov chains",
  "main": "./bin/main",
  "preferGlobal": true,
  "bin": {
    "sobriquet": "./dist/main.js"
  },
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-eslint": "^4.1.6",
    "babel-jest": "^5.0.0",
    "babel-loader": "^5.4.0",
    "command-line-args": "^2.1.3",
    "jest-cli": "^0.8.1",
    "webpack": "^1.12.9"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "compile": "webpack --colors --progress",
    "dev": "npm run test -- --watch",
    "start": "node ./dist/main.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/restack/sobriquet.git"
  },
  "author": "Restack Cloud <contact@restackcloud.com>",
  "license": "MIT",
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  }
}

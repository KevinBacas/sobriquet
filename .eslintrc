{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-confusing-arrow": 0,
    "react/jsx-quotes": 0,
    "jsx-quotes": [2, "prefer-double"],
    "new-cap": 0,
    "prefer-template": 0,
    "react/prefer-stateless-function": 0,
    "no-console": 1,
  }
}
